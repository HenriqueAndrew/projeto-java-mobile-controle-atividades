<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.henriqueandrew.controleatividades">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ControleAtividades">

        <activity
            android:name=".apresentacao.HomeActivity"
            android:parentActivityName=".apresentacao.HistoricoProfActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".apresentacao.HistoricoProfActivity" />
        </activity>

        <activity android:name=".apresentacao.HistoricoProfActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".apresentacao.CadastroActivity"
            android:parentActivityName=".apresentacao.HistoricoProfActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".apresentacao.HistoricoProfActivity" />
        </activity>

    </application>

</manifest>